{
  "name": "chaos-equations",
  "version": "1.1.6",
  "description": "Visualization and animation of real time dynamic chaos equations",
  "author": "Jered Danielson <jered@uw.edu>",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "build": "rm -rf dist && parcel build src/index.html",
    "dev": "parcel src/index.html",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --validate --sourceMap --debug",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --validate --sourceMap --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "test": "node tests"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.9.2",
    "build": "^0.1.4",
    "express": "^4.17.1",
    "js-yaml": "^3.13.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "reset-css": "^4.0.1",
    "stats-js": "^1.0.1",
    "three": "^0.103.0"
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/chaos-equations"
  },
  "keywords": [
    "node",
    "glitch",
    "express",
    "parcel",
    "react",
    "react-router",
    "chaos",
    "animations",
    "visualizations",
    "three.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "assemblyscript": "^0.9.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1"
  },
  "staticFiles": {
    "staticPath": "build",
    "watcherGlob": "**"
  }
}
